@model SoccerLeague.Models.ComissaoTecnica

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Editar Membro da Comissão Técnica";
}

<h2>Editar Membro da Comissão Técnica</h2>

@using (Html.BeginForm("Edit", "ComissaoTecnica", FormMethod.Post))
{
    @Html.AntiForgeryToken()
    @Html.HiddenFor(m => m.ComissaoTecnicaId)

    @Html.Partial("_ComissaoTecnicaForm", Model)

    <button type="submit" class="btn btn-primary">Salvar</button>
    @Html.ActionLink("Voltar", "Index", null, new { @class = "btn btn-default" })
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
